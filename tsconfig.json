{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "resolveJsonModule": true,
    "outDir": "dist",
  "rootDir": "./src",
    "baseUrl": "./",
  },
  "include": ["./"],
  "exclude": ["node_modules", "dist"]
}